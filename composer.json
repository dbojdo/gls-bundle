{
    "name" : "webit/gls-bundle",
    "type": "symfony-bundle",
    "description": "GLS ADE and GLS Track & Trace API Symfony 2 integration",
    "keywords": ["web-it","shipment", "bundle", "delivery", "parcel", "dispatch", "gls", "gls-ade", "track", "gls-track"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "^2.3",
        "webit/gls-ade": "^2.0.0",
        "webit/gls-tracking": "^2.0.0"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "phpunit/phpunit": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\GlsBundle\\" : "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
